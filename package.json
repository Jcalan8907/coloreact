  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.7.4",
    "babel-loader": "7.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.2",
    "prop-types": "15.5.10",
    "react": "15.6.2",
    "react-dom": "15.6.1",
    "react-syntax-highlighter": "5.6.2",
    "style-loader": "0.18.2",
    "webpack": "2.6.1",
